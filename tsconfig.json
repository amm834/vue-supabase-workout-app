{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "node_modules/.pnpm/vue@3.2.39/node_modules/vue/macros-global.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "isolatedModules": false,
    "importsNotUsedAsValues": "remove"
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}
